    <ProjectReference Include="..\..\Libraries\Goedel.Mesh.Platform.Windows\Goedel.Mesh.Platform.Windows.csproj" />
    <ProjectReference Include="..\..\Libraries\Goedel.Mesh.Shell\Goedel.Mesh.Shell.csproj" />
    <ProjectReference Include="..\..\Libraries\Goedel.Mesh\Goedel.Mesh.csproj" />
    <ProjectReference Include="..\..\Libraries\Goedel.Persistence\Goedel.Persistence.csproj" />
    <ProjectReference Include="..\..\Libraries\Goedel.Protocol\Goedel.Protocol.csproj" />
    <ProjectReference Include="..\..\Protocols\Async\Goedel.Async.Client\Goedel.Async.Client.csproj" />
    <ProjectReference Include="..\..\Protocols\Async\Goedel.Async\Goedel.Async.csproj" />
    <ProjectReference Include="..\..\Protocols\Catalog\Goedel.Catalog.Client\Goedel.Catalog.Client.csproj" />
